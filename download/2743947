                <button type="button" class="mdc-button mdc-button--unelevated nls-button nls-toggle" data-button-id="unsub"><span class="mdc-button__label nls-button__label">Abonniert</span><span class="material-icons icon-arrow-drop-down">arrow_drop_down</span></button><button type="button" class="mdc-button nls-button nls-field hidden nl-unsubscribe" data-button-id="logout"><span class="mdc-button__label nls-button__label">Nicht mehr abonnieren</span></button>

        

            {{else}}

        

                <button type="button" class="mdc-button mdc-button--unelevated nls-button" data-button-id="unsub" disabled><span class="material-icons icon-done">done</span><span class="mdc-button__label nls-button__label">Abonniert</span></button>

        

