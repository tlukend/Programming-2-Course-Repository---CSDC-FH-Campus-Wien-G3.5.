                    window.KRNAuthSuccessTriggerCall = 'krn-privacy-center-open-purchase';
                    self.log('setting KRNAuthCloseTriggerCall');
                    window.KRNAuthCloseTriggerCall = 'krn-privacy-center-purchase-cancelled';
                },
                unsetTriggerCalls: function() {
