            // reload viewable refreshable slots
            if(window.GPTadSlots.refreshableSlots.length > 0) {
                googletag.pubads().addEventListener('impressionViewable', function(event) { // impression gets viewable after being visible once
                    reloadRefreshableSlots(event);
                });
