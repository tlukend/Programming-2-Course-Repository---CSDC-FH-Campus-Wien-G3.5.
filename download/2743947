"){KRNModalManager.load("nl_sub_info",function(){if(window.TRINITY_LIGHT.user){$("#krn-nl-sub-info").modal("show");$("#krn-nl-sub-info").find($("#nl-profile-link")).attr("href",window.location.origin+"/profil/"+window.TRINITY_LIGHT.getID()+"/benachrichtigungen")}else{$("#krn-nl-sub-info-anon").modal("show")}})}})},setupClickHandlers:function(type){var self=this;if(type==="normal"){self.boxes.each(function(){var b=$(this);$(this).find(".nls-toggle").off("click").on("click",function(){self.boxes.each(function(){$(this).find(".nls-field").toggleClass("hidden");$(this).find(".icon-arrow-drop-down").toggleClass("rotate")})});$(this).find(".nl-subscribe").off("click").on("click",function(){var mail=b.find(".nl-mail");var flag=b.data("optin-flag-id");self.subscribe(mail,flag,b,false)});$(this).find(".nl-unsubscribe").off("click").on("click",function(){var flag=b.data("optin-flag-id");if($(document).find($("#krn-nl-unsub-info")).length==0){KRNModalManager.load("nl_sub_info",function(){$("#krn-nl-unsub-info").data("of",flag);$("#krn-nl-unsub-info").modal("show");krnNlSubInfo("unsub")})}else{$("#krn-nl-unsub-info").data("of",flag);$("#krn-nl-unsub-info").modal("show")}b.find("button").attr("disabled",true)})})}if(type==="article"){self.articleBoxes.each(function(){var b=$(this);$(this).find(".nl-subscribe").off("click").on("click",function(){var mail=b.find(".nl-mail");var flag=b.data("optin-flag-id");self.subscribe(mail,flag,b,true)})})}},loadSubscriptionInfo:function(){var self=this;var apiUrl="//"+self.apiHost+"/v1/newsletter/subs";if(!window.TRINITY_LIGHT.user&&Cookies.get("nl-mail-address")){apiUrl+="?mail_address="+encodeURIComponent(Cookies.get("nl-mail-address"))}if(window.TRINITY_LIGHT.user){apiUrl+="?mail_address="+encodeURIComponent(window.TRINITY_LIGHT.getEmail())}if(window.TRINITY_LIGHT.user||Cookies.get("nl-mail-address")){$.post(apiUrl,function(d){self.render(d)})}else{self.render({})}},render:function(data){var self=this;if(typeof self.boxes!=="undefined"){self.boxes.each(function(){var title=$(this).data("title");var description=$(this).data("description");var image=$(this).data("image");var interval=$(this).data("interval");var flag=$(this).data("optin-flag-id");var data_obj={email:Cookies.get("nl-mail-address"),subs:false,logged_in:window.TRINITY_LIGHT.user?true:false,image:image,interval:interval,title:title,description:description,flag:flag};var template="subscribe";if(data.emarsys){data_obj.status=data.emarsys.optin;if(data.emarsys.optin_flags&&data.emarsys.optin_flags[flag]==1){data_obj.subs=true}if(data_obj.subs===true&&data_obj.status==null){template="pending"}if(data_obj.subs===true&&data_obj.status==1){template="unsubscribe"}}$(this).html(self.templates[template](data_obj));self.setupClickHandlers("normal");mdcInits.init(".c_newsletter-signup")})}if(typeof self.articleBoxes!=="undefined"){self.articleBoxes.each(function(){var title=$(this).data("title");var description=$(this).data("description");var image=$(this).data("image");var flag=$(this).data("optin-flag-id");var data_obj={email:Cookies.get("nl-mail-address"),subs:false,logged_in:window.TRINITY_LIGHT.user?true:false,image:image,title:title,description:description,flag:flag};var template="subscribeArticle";if(data.emarsys){data_obj.status=data.emarsys.optin;if(data.emarsys.optin_flags&&data.emarsys.optin_flags[flag]==1){data_obj.subs=true}if(data_obj.subs===true&&data_obj.status==null){template="pendingArticle"}if(data_obj.subs===true&&data_obj.status==1){template="subscribedArticle"}}$(this).closest($(".c_newsletter-signup-article")).show();$(this).html(self.articleTemplates[template](data_obj));self.setupClickHandlers("article");mdcInits.init(".c_newsletter-signup-article")})}},renderPlaceHolders:function(){var self=this;if(typeof self.boxes!=="undefined"){self.boxes.each(function(){$(this).html(Handlebars.compile($("#krn-nl-placeholder").html()))})}if(typeof self.articleBoxes!=="undefined"){self.articleBoxes.each(function(){$(this).html(Handlebars.compile($("#krn-nl-placeholder").html()))})}}};$(document).ready(function(){KRNEvents.on("trinity-light-loaded",function(){KRNNLManager.init()});KRNEvents.on("krn-login",function(){KRNNLManager.init()});KRNEvents.on("krn-logout",function(){KRNNLManager.init()})});function initOneGenericSlickSlider($el){var $par=$el;if(!$par.length){return false}if($par.data("init")){return false}var desktop=$el.data("desktop");var mobile=$el.data("mobile");$par.wrapInner('<div class="slickslider"></div>');$par.find(".box").wrap("<div class='row'></div>");var prevArrow=false;var nextArrow=false;var infinite=false;var dots=true;if($el.data("show-arrows")&&!kmmIsMobile()){switch($el.data("design")){case"arrows_top":$par.append('<div class="slickslider__prev"><span class="material-icons">chevron_left</span></div>'+'<div class="slickslider__next"><span class="material-icons">chevron_right</span></div>');break;default:$par.append('<div class="slickslider__prev"></div>'+'<div class="slickslider__next"></div>');break}prevArrow=$par.find(".slickslider__prev");nextArrow=$par.find(".slickslider__next");infinite=true}if($el.data("show-dots")){dots=true}else{dots=false}if(kmmIsMobile()){infinite=true}var defaults={prevArrow:prevArrow,nextArrow:nextArrow,dots:dots,infinite:infinite,variableWidth:true,useTransform:false};var desktopConfig={slidesToShow:desktop,slidesToScroll:desktop};var mobileConfig={slidesToShow:mobile,slidesToScroll:mobile};var bundleConfig=kmmIsMobile()?mobileConfig:desktopConfig;var extendedConfig={};$par.find(".slickslider").slick($.extend({},defaults,bundleConfig,extendedConfig));$par.attr("data-init","true")}$(document).ready(function(){setTimeout(function(){$(".c_slickslider .slickslider_root").css("display","none");KRNEvents.on("krn-slick-carousel-js-loaded",function(){$(".c_slickslider .slickslider_root").each(function(){initOneGenericSlickSlider($(this));$(".c_slickslider .slickslider_root").css("display","block")})})},100)});$(document).ready(function(){KRNSocialToolbar.init()});$(document).ready(function(){mdcInits.init(".c_top-bar");KRNTopBarSearch.init();KRNTopBarRegio.init()});$(document).ready(function(){KRNWidgetWeatherBox.init()});
            </script></body> </html>