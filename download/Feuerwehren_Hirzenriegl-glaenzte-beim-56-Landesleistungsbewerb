    <div class="hidden">
     <script>
        const throttle = (callback, limit) => {
          let tick = false;

          return function () {
            if (!tick) {
              tick = true;

              setTimeout(function () {
                callback.call();
                tick = false;
              }, limit);
            }
          }
        }

        const onScroll = () => {
          const state = (document.documentElement.scrollTop > 32)
            ? "scrolled"
            : "init";

          document.getElementById("js-body").setAttribute("scroll-state", state);
        };

        window.addEventListener("scroll", throttle(onScroll, 80));
      </script>
    </div>

