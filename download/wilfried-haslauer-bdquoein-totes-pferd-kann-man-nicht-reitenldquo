<script>
let windowsDevices = false;

// If it's a windows phone or some mobile IE browser or windows phone in desktop mode!
if(navigator.userAgent.match(/Windows Phone/i) || navigator.userAgent.match(/iemobile/i) || navigator.userAgent.match(/WPDesktop/i)){
    windowsDevices = true
}

if(!window.sessionStorage.getItem('iewarning') && window.document.documentMode || windowsDevices) {
    document.getElementById('ie-warning').style.display = 'flex';
}
function closeIEWarning() {
    document.getElementById('ie-warning').style.display = 'none';
    window.sessionStorage.setItem('iewarning', 'true');
}
</script>
